{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jest",
    "jasmine"
  ],
  "env": {
    "jest/globals": true,
    "jasmine": true
  },
  "rules": {
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}
